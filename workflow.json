{
    "ComputeServers": {
        "My_GitHub_Account": {
            "FaaSType": "GitHubActions",
            "UserName": "IzzyLerman",
            "UseSecretStore": false,
            "ActionRepoName": "FaaSr-py-testing",
            "Branch": "main",
            "Token": "My_GitHub_Account_TOKEN"
        },
        "My_Lambda_Account": {
            "FaaSType": "Lambda",
            "Region": "us-east-1",
            "AccessKey": "My_Lambda_Account_ACCESS_KEY",
            "SecretKey": "My_Lambda_Account_SECRET_KEY"
        },
          "My_OW_Account": {
            "FaaSType": "OpenWhisk",
            "Namespace": "guest",
            "AllowSelfSignedCertificate": true,
            "Endpoint": "ow.faasr.io:31002",
            "API.key": "My_OW_Account_API_KEY"

        }
    },
    "DataStores": {
        "My_S3_Bucket": {
            "Bucket": "faasr-py-testing",
            "Region": "us-east-1",
            "Writable": "TRUE",
            "AccessKey": "My_S3_Bucket_ACCESS_KEY",
            "SecretKey": "My_S3_Bucket_SECRET_KEY"
        }
    },
    "ActionList": {
        "add_operation": {
            "FunctionName": "add_rpc",
            "FaaSServer": "My_OW_Account",
            "Type": "Python",
            "Arguments": {
                "folder": "testadd",
                "num1": 400,
                "num2": 100,
                "output": "add_result_500.txt"
            },
            "InvokeNext": [
                "r_func_new"
            ]
        },
        "r_func_new": {
            "FunctionName": "r_log",
            "FaaSServer": "My_OW_Account",
            "Type": "R",
            "Arguments": {
                "message": "testing testing testing"
            },
            "InvokeNext": [
                "delete"
            ]
        },
        "delete": {
            "FunctionName": "print_and_delete",
            "FaaSServer": "My_OW_Account",
            "Type": "Python",
            "Arguments": {
                "folder": "testadd",
                "filename": "add_result_500.txt"
            },
            "InvokeNext": [
                {"True": ["folders"]}
            ]
        },
        "folders": {
            "FunctionName": "test_list_folders",
            "FaaSServer": "My_OW_Account",
            "Type": "Python",
            "Arguments": {},
            "InvokeNext": []
        }
    },
    "ActionContainers": {
        "r_func_new": "docker.io/izzylerman/openwhisk-r:0.4",
        "add_operation": "docker.io/izzylerman/openwhisk:0.4",
        "delete": "docker.io/izzylerman/openwhisk:0.4",
        "folders": "docker.io/izzylerman/openwhisk:0.4"
    },
    "FunctionCRANPackage": {
        "r_log": [
            "gapminder"
        ]
    },
    "PyPIPackageDownloads": {
        "add_rpc": [
            "random2"
        ]
    },
    "FunctionGitHubPackage": {
        "add_rpc": [
            "oittaa/uuid6-python"
        ]
    },
    "FunctionGitRepo": {
        "add_rpc": "izzylerman/test-py",
        "r_log": "izzylerman/test-py",
        "print_and_delete": "izzylerman/test-py",
        "test_list_folders": "izzylerman/test-py"
    },
    "FunctionInvoke": "add_operation",
    "InvocationID": "",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_S3_Bucket",
    "DefaultDataStore": "My_S3_Bucket",
    "WorkflowName": "test-workflow"
}
